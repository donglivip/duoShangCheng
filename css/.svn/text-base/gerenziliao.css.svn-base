*{padding: 0;margin: 0;}
html,body,.wrapper{width: 100%;height: 100%;}
.wrapper{
	background: #F4F7FA ;
}
.header{
	height: .96rem;
	background: #FFFFFF;
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 0 .3rem ;
	margin-bottom: .1rem;
}
.header-aside{
	width: .3rem;
	display: flex;
	align-items: center;
	justify-content: center;
}
.header-aside img{
	height: .36rem;
}
.header-text{
	font-size: .36rem;
	color: #141414 ;
}
.main-box{
	height: 3.7rem;
	padding: 0 .3rem ;
	background: #FFFFFF;
}
.main-top{
	display: flex;
	align-items: center;
	justify-content: flex-start;
	height: 1.6rem;
}
.img-box{
	height: 1rem;
	width: 1rem;
	overflow: hidden;
}
.img-box img{
	width: 100%;
	border-radius: 2rem;
}
.top-text{
	font-size: .28rem;
	color: #929292 ;
	flex: 1;
	margin-left: .4rem;
}
.youjian{
	height: .36rem;
}
.main-content{
	display: flex;
	align-items: center;
	justify-content: flex-start;
	height: .9rem;
}
.content-lift{
	font-size: .28rem;
	color: #929292 ;
	width: 1.3rem;
	margin-right: .6rem;
}
.content-text{
	font-size: .28rem;
	color: #272727 ;
	flex: 1;
}
.main-content img{
	height: .36rem;
}
