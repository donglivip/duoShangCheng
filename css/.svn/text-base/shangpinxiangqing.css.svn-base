*{padding: 0;margin: 0;}
html,body,.wrapper{width: 100%;height: 100%;}
.wrapper{
	background: #F8F8F8 ;
}
.header{
	display: flex;
	align-items: center;
	position: fixed;
	top: .2rem;
	left: .2rem;
}
.header img{
	height: .64rem;
}
.header02{
	right: .2rem;background: rgba(0,0,0,.5);border-radius: 50%;width: .6rem;height: .6rem;display: flex;align-items: center;justify-content: center;position: fixed;top: .2rem;right: .2rem;
}
.main{
	height: calc(100% - 1.3rem);
	overflow-x: hidden;
	overflow-y: scroll;
}
.main-one {
	height: 5.6rem;
	width: 7.5rem;
	overflow: hidden;
}
.main-one img{
	width: 100%;
}
.main-two{
	height: 3rem;
	background: #FFFFFF;
	margin-bottom: .2rem;
}
.two-top{
	display: flex;
	align-items: center;
}
.two-text{
	font-size: .34rem;
	color: #F55353 ;
	margin: .3rem;
	font-weight: 600 ;
}
.two-content{
	padding: 0 .3rem ;
}
.two-news{
	font-size: .32rem;
	color: #000000 ;
	height: .88rem;
	line-height: .44rem;
	overflow: hidden;
	margin-bottom: .4rem;
}
.two-bottom{
	display: flex;
	align-items: center;
	justify-content: space-between;
	padding: 0 .3rem;
}
.two-word{
	font-size: .28rem;
	color: #9B9B9B ;
}
.main-three{
	height: 2rem ;
	background: #FFFFFF;
	padding: 0 .3rem;
	margin-bottom: .2rem;
}
.three-box{
	display: flex;
	align-items: center;
	justify-content: flex-start;
	height: 1rem;
	border-bottom: 1px solid #F3F3F3 ;
}
.three-text{
	font-size: .28rem;
	color: #9B9B9B ;
	margin-right: .6rem;
}
.three-news{
	font-size: .28rem;
	color: #141414 ;
	flex: 1;
}
.three-box img{
	height: .36rem;
}
.three-hezi{
	display: flex;
	align-items: center;
	justify-content: flex-start;
	height: 1rem;
}
.main-four{
	height: 4.2rem;
	background: #FFFFFF;
	padding: 0 .3rem;
	margin-bottom: .2rem;
}
.four-top{
	display: flex;
	align-items: center;
	justify-content: space-between;
	height: 1rem;
}
.four-text{
	font-size: .32rem;
	color: #141414 ;
}
.four-top img{
	height: .36rem;
}
.four-content{
    display: flex;
	align-items: center;
	justify-content: flex-start;
	height: 1rem;
}
.img-hezi{
	width: .64rem;
	height: .64rem;
	overflow: hidden;
	display: flex;
}
.four-content img{
	width: 100%;
	border-radius: 2rem;
}
.four-news{
	font-size: .28rem;
	color: #141414 ;
	margin-left: .12rem;
}
.four-bottom{
	margin-left: 1rem;
}
.four-upper{
	font-size: .28rem;
	color: #141414 ;
	margin-bottom: .2rem;
}
.four-lower{
	display: flex;
	align-items: center;
	justify-content: flex-start;
}
.img-box{
	width: 1.4rem;
	height: 1.4rem;
	overflow: hidden;
	margin-right: .2rem;
}
.four-lower img{
	width: 100%;
	border-radius: .12rem ;
}
.main-five{
	height: 1rem;
	background: #FFFFFF;
	padding: 0 .3rem;
	display: flex;
	align-items: center;
	justify-content: flex-start;
}
.five-text{
	font-size: .32rem;
	color: #141414 ;
}
.bottom{
	height: 1.3rem;
	background: #FFFFFF;
	display: flex;
	align-items: center;
	justify-content: flex-start;
}
.bottom-lift{
	width: .6rem;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	margin: 0 .8rem;
}
.bottom-lift img{
	height: .4rem;
	margin-bottom: .06rem;
}
.bottom-text{
	font-size: .2rem;
	color: #141414 ;
}
.bottom-right{
	display: flex;
	align-items: center;
	justify-content: center;
}
.bottom-news{
	font-size: .28rem;
	color: #FFFFFF ;
	width: 2.3rem;
	height: .8rem;
	background: #FF8686 ;
	text-align: center;
	line-height: .8rem;
	border-top-left-radius: .4rem;
	border-bottom-left-radius: .4rem;
}
.bottom-word{
	font-size: .28rem;
	color: #FFFFFF ;
	width: 2.3rem;
	height: .8rem;
	background: #F55353  ;
	text-align: center;
	line-height: .8rem;
	border-top-right-radius: .4rem;
	border-bottom-right-radius: .4rem;
}









